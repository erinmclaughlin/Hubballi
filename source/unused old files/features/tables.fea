# Horizontal header
# -----------------

table hhea {
  Ascender 634;
  Descender -366;
  LineGap 0;
} hhea;


# OS/2 and Windows metrics
# ------------------------

table OS/2 {
  FSType 0;
  Panose 2 0 0 0 0 0 0 0 0 0;
  UnicodeRange
    0   # Basic Latin
    1   # Latin-1 Supplement
    # 2   # Latin Extended-A
    # 15  # Devanagari 0900-097F
    # 16  # Bengali 0980-09FF
    # 17  # Gurmukhi 0A00-0A7F
    # 18  # Gujarati 0A80-0AFF
    # 19  # Oriya 0B00-0B7F
    # 20  # Tamil 0B80-0BFF
    # 21  # Telugu 0C00-0C7F
    22  # Kannada 0C80-0CFF
    # 23  # Malayalam 0D00-0D7F
    ;
  # CodePageRange;
  TypoAscender 634;
  TypoDescender -366;
  TypoLineGap 0;
  winAscent 634;
  winDescent 366;
  ###include (weightclass.fea); # WeightClass 400;
  WidthClass 5;
  Vendor "GOOG";
} OS/2;


